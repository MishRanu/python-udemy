fruit = {"orange": "a sweet organge citrux fruit",
         "apple": "good for making cider",
         "grape": "a small sweet fruit",
         "lime": "a sour green citrus fruit"}

print(fruit)

veg = {"cabbage": "every child's favorite",
       "sprouts": "good for health",
       "spinach": "can i have some more fruit, please"
       }
print(veg)
#Yeah the two dictionareies got combined
# veg.update(fruit)
# print(veg)
# print(fruit.update(veg))
# print(fruit)
# print(fruit==veg)

nice_and_nasty = fruit.copy()
nice_and_nasty.update(veg)
print(nice_and_nasty)
print(veg)
print(fruit)
#better to use the copy method rather than update

